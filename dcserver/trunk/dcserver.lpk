<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="5">
    <Name Value="DCServer"/>
    <Type Value="RunTimeOnly"/>
    <BuildMethod Value="Both"/>
    <Author Value="Joost van der Sluis / CNOC"/>
    <CompilerOptions>
      <Version Value="11"/>
      <SearchPaths>
        <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)"/>
      </SearchPaths>
    </CompilerOptions>
    <Description Value="Framework for building applications which have to process different commands one-by-one. Commands can be initiated and monitored in several ways, like from the comment-line or by tcp/ip-connected clients."/>
    <License Value="LGPL with modification"/>
    <Version Minor="9" Release="3" Build="1"/>
    <Files>
      <Item>
        <Filename Value="dcshandler.pas"/>
        <UnitName Value="dcsHandler"/>
      </Item>
      <Item>
        <Filename Value="dcsconsoleserver.pas"/>
        <UnitName Value="dcsConsoleServer"/>
      </Item>
      <Item>
        <Filename Value="dcsinoutputprocessor.pas"/>
        <UnitName Value="dcsInOutputProcessor"/>
      </Item>
      <Item>
        <Filename Value="dcsthreadcommandfactory.pas"/>
        <UnitName Value="dcsThreadCommandFactory"/>
      </Item>
      <Item>
        <Filename Value="dcstcpserver.pas"/>
        <UnitName Value="DCSTCPServer"/>
      </Item>
      <Item>
        <Filename Value="dcsclientthread.pas"/>
        <UnitName Value="DCSClientThread"/>
      </Item>
      <Item>
        <Filename Value="dcslistenerthread.pas"/>
        <UnitName Value="dcsListenerThread"/>
      </Item>
      <Item>
        <Filename Value="dcshttprestserver.pas"/>
        <UnitName Value="DCSHTTPRestServer"/>
      </Item>
      <Item>
        <Filename Value="dcsglobalsettings.pas"/>
        <UnitName Value="dcsGlobalSettings"/>
      </Item>
    </Files>
    <RequiredPkgs>
      <Item>
        <PackageName Value="pthreads"/>
        <DependencyType Value="FPMake"/>
      </Item>
      <Item>
        <PackageName Value="fcl-web"/>
        <DependencyType Value="FPMake"/>
      </Item>
      <Item>
        <PackageName Value="fcl-net"/>
        <DependencyType Value="FPMake"/>
      </Item>
      <Item>
        <PackageName Value="fcl-process"/>
        <DependencyType Value="FPMake"/>
      </Item>
      <Item>
        <PackageName Value="fcl-json"/>
        <DependencyType Value="FPMake"/>
      </Item>
      <Item>
        <PackageName Value="cnocThreadingTools"/>
        <DependencyType Value="FPMake"/>
      </Item>
      <Item>
        <PackageName Value="fcl-base"/>
        <DependencyType Value="FPMake"/>
      </Item>
    </RequiredPkgs>
    <UsageOptions>
      <UnitPath Value="$(PkgOutDir)"/>
    </UsageOptions>
    <PublishOptions>
      <Version Value="2"/>
    </PublishOptions>
  </Package>
</CONFIG>
